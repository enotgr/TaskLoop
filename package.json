{
  "name": "task-loop-app",
  "version": "1.0.0",
  "description": "TaskLoop app with angular and express",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix task-loop-client",
